   "source": [
    "# Python Decorator"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "0f477afc-1259-4ed2-9a2d-670e6e9e59a8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def uppercase(func):\n",
    "    def wrapper():\n",
    "        f = func()\n",
    "        uppered = f.upper()\n",
    "        return uppered\n",
    "    return wrapper\n",
    "\n",
    "@uppercase\n",
    "def hello():\n",
    "    return \"hello world!\""
   ]
  },
